@Article{Shelepova2005,
  author          = {Shelepova, Tatiana and Nafziger, Anne N and Victory, Jennifer and Kashuba, Angela D M and Rowland, Elizabeth and Zhang, Yanhua and Sellers, Edward and Kearns, Gregory and Leeder, J Steven and Gaedigk, Andrea and Bertino, Joseph S},
