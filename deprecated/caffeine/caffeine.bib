  issue           = {1},
  keywords        = {Citalopram, blood, pharmacokinetics; Clinical Trials as Topic; Drug Interactions; Fluoxetine, blood, pharmacokinetics; Fluvoxamine, blood, pharmacokinetics; Humans; Paroxetine, blood, pharmacokinetics; Serotonin Uptake Inhibitors, blood, pharmacokinetics; Sertraline, blood, pharmacokinetics},
  nlm-id          = {7905840},
