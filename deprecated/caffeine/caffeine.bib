  keywords        = {Adult; Anti-Ulcer Agents, pharmacology; Caffeine, antagonists & inhibitors, pharmacokinetics; Central Nervous System Stimulants, antagonists & inhibitors, pharmacokinetics; Chromatography, High Pressure Liquid; Cimetidine, pharmacology; Female; Humans; Liver, metabolism; Longitudinal Studies; Male; Metabolic Clearance Rate; Pilot Projects; Prospective Studies; Saliva, chemistry, metabolism},
  nlm-id          = {8111305},
