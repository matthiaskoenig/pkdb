  issn-linking    = {0960-314X},
  issue           = {4},
  keywords        = {Benzoflavones, pharmacology; Biomarkers; Caffeine, metabolism; Cytochrome P-450 CYP1A2; Cytochrome P-450 Enzyme Inhibitors; Cytochrome P-450 Enzyme System, genetics, metabolism; DNA, genetics; Humans; In Vitro Techniques; Kinetics; Microsomes, Liver, enzymology; Oxidoreductases, N-Demethylating, antagonists & inhibitors, genetics, metabolism},
  nlm-id          = {9211735},
