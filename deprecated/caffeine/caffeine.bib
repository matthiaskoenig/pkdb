  journal-abbreviation = {Pharmacogenetics},
  keywords             = {Adolescent; Adult; Biotransformation; Caffeine, metabolism; Central Nervous System Stimulants, metabolism; Chromatography, High Pressure Liquid; Contraceptives, Oral, Hormonal, adverse effects; Cytochrome P-450 CYP1A2, metabolism; Female; Genetic Predisposition to Disease; Humans; Male; Middle Aged; Sex Factors; Smoking, metabolism; Twins, Dizygotic, genetics; Twins, Monozygotic, genetics},
  nlm-id               = {9211735},
