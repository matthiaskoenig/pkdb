@Article{Nakajima1994,
  author          = {Nakajima, M and Yokoi, T and Mizutani, M and Shin, S and Kadlubar, F F and Kamataki, T},
  title           = {Phenotyping of CYP1A2 in Japanese population by analysis of caffeine urinary metabolites: absence of mutation prescribing the phenotype in the CYP1A2 gene.},
