@Article{Harder1988,
  author          = {Harder, S and Staib, A H and Beer, C and Papenburg, A and Stille, W and Shah, P M},
  title           = {4-quinolones inhibit biotransformation of caffeine.},
  journal         = {European journal of clinical pharmacology},
  year            = {1988},
  volume          = {35},
  pages           = {651--656},
  issn            = {0031-6970},
  abstract        = {The pharmacokinetics of caffeine, including formation of its major metabolite paraxanthine in plasma, has been investigated in 12 healthy males (age 20-40 years) alone and during co-administration of the 4-quinolones ofloxacin, norfloxacin, pipemidic acid, ciprofloxacin, and enoxacin; ciprofloxacin and enoxacin were given in 3 different dose levels. The naphthyridine derivative enoxacin and the pyrido-pyrimidine derivative pipemidic acid had caused marked inhibition of caffeine and paraxanthine metabolism, whereas the genuine quinolone derivatives norfloxacin and ciprofloxacin had little effect, and the pyrido-benzoxacine derivative ofloxacin had no detectable effect. The different molecular and spatial structures of the compounds appear to be responsible for the differences in inhibitory potency.},
