  issue           = {4A},
  keywords        = {4-Quinolones; Anti-Infective Agents, pharmacology; Caffeine, pharmacokinetics; Drug Interactions; Humans; Theophylline, pharmacokinetics; Xanthines, pharmacokinetics},
  nlm-id          = {0267200},
  owner           = {NLM},
