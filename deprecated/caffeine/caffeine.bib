  issue           = {4},
  keywords        = {Adolescent; Adrenergic alpha-Agonists, blood, pharmacokinetics, pharmacology; Adult; Area Under Curve; Caffeine, blood; Clonidine, analogs & derivatives, blood, pharmacokinetics, pharmacology; Contraceptives, Oral, Combined, pharmacology; Cytochrome P-450 CYP1A2 Inhibitors; Drug Synergism; Ethinyl Estradiol, pharmacology; Female; Half-Life; Humans; Norpregnenes, pharmacology; Theophylline, blood},
  nlm-id          = {0372741},
  owner           = {NLM},
  pii             = {S0009-9236(05)00279-1},
  pmid            = {16198659},
