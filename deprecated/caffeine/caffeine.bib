  issue           = {12},
  keywords        = {Animals; Caffeine, metabolism; Cytochrome P-450 CYP1A2, metabolism; Humans; Mice; Phosphodiesterase Inhibitors, metabolism; Species Specificity; Theophylline, blood, metabolism, urine},
  nlm-id          = {0101032},
