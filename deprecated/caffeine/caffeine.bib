  country         = {United States},
  created         = {1985-06-19},
  issn-linking    = {0090-9556},
  issue           = {2},
  keywords        = {Animals; Blood Proteins, metabolism; Caffeine, metabolism; Injections, Intravenous; Kinetics; Male; Protein Binding; Rats; Theophylline, administration & dosage, blood, metabolism},
  nlm-id          = {9421550},
