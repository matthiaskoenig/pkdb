  issn-linking    = {0142-2782},
  issue           = {6},
  keywords        = {Databases, Bibliographic; Glucuronosyltransferase, metabolism; Humans; Liver, metabolism; abundance; correlation of expression; in vitro-in vivo extrapolation; inter-individual variability; uridine 5′-diphosphate glucuronosyltransferase},
  nlm-id          = {7911226},
