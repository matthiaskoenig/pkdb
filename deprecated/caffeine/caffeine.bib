@Article{Berthou1991,
  author          = {Berthou, F and Flinois, J P and Ratanasavanh, D and Beaune, P and Riche, C and Guillouzo, A},
  title           = {Evidence for the involvement of several cytochromes P-450 in the first steps of caffeine metabolism by human liver microsomes.},
