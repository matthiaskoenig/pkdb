  issn-linking    = {0306-5251},
  issue           = {1},
  keywords        = {Administration, Oral; Adult; Caffeine, metabolism; Drug Interactions; Drug Tolerance; Female; Humans; Kinetics; Male; Sex Factors; Theophylline, adverse effects, analogs & derivatives, pharmacology},
  nlm             = {PMC1386134},
